{
  "name": "senarai",
  "private": false,
  "sideEffects": false,
  "prisma": {
    "seed": "node --require esbuild-register prisma/seed.ts"
  },
  "scripts": {
    "generate:css": "npx tailwindcss -o ./app/tailwind.css",
    "generate:prisma": "prisma generate --schema=./prisma/schema.sqlite.prisma",
    "reset:prisma": "prisma migrate reset --force --schema=./prisma/schema.sqlite.prisma",
    "build": "run-s build:*",
    "build:fetch": "npm run fetch",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "dev": "npm run generate:prisma && run-p dev:*",
    "dev:css": "npm run generate:css -- --watch",
    "dev:remix": "remix dev",
    "fetch": "node --require esbuild-register app/scripts/fetch-db.ts",
    "format": "pkode format",
    "lint": "pkode lint",
    "setup": "npm install && npm run fetch && cp -r .env.example .env && npm run reset:prisma",
    "setup:e2e": "npm run fetch && cross-env DATABASE_URL=file:./test.db npm run reset:prisma && npm run build:css",
    "start:e2e": "cross-env DATABASE_URL=file:./test.db npm run dev:remix",
    "test:e2e": "playwright test",
    "test:e2e:report": "playwright show report",
    "test": "is-ci-cli test:coverage test:watch",
    "test:watch": "vitest",
    "test:verbose": "vitest --reporter=verbose",
    "test:coverage": "vitest --coverage",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@headlessui/react": "1.7.2",
    "@prisma/client": "4.3.1",
    "@remix-run/node": "1.12.0",
    "@remix-run/react": "1.12.0",
    "@remix-run/vercel": "1.12.0",
    "@vercel/node": "2.9.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.8.0"
  },
  "devDependencies": {
    "@faker-js/faker": "7.5.0",
    "@jackfranklin/test-data-bot": "2.0.0",
    "@playwright-testing-library/test": "4.4.0",
    "@playwright/test": "1.27.1",
    "@remix-run/dev": "1.12.0",
    "@remix-run/eslint-config": "1.12.0",
    "@remix-run/serve": "1.12.0",
    "@tailwindcss/forms": "0.5.3",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "14.4.3",
    "@types/jsdom": "20.0.0",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "@types/tailwindcss": "3.1.0",
    "@vitejs/plugin-react": "2.1.0",
    "@vitest/coverage-c8": "0.23.4",
    "chalk": "5.0.1",
    "cheerio": "1.0.0-rc.12",
    "cross-env": "7.0.3",
    "cross-fetch": "3.1.5",
    "esbuild": "0.15.8",
    "esbuild-register": "3.3.3",
    "is-ci-cli": "2.2.0",
    "jsdom": "20.0.0",
    "npm-run-all": "4.1.5",
    "ora": "6.1.2",
    "pkode": "1.5.1",
    "prisma": "4.3.1",
    "tailwindcss": "3.1.8",
    "typescript": "^4.7.4",
    "vitest": "0.23.4"
  },
  "engines": {
    "node": ">=14"
  }
}
